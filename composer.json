    "homepage": "https://woocommerce.com/products/woocommerce-gateway-stripe/",
		"license": "GPL-2.0",
		"config": {
      "platform": {
        "php": "7.0"
      }
    },
    "require-dev": {
				"composer/installers": "1.9.0",
